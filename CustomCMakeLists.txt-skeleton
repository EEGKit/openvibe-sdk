# This file is an example configuration of OpenViBE build system
# By default all changes are disabled, you can un-comment any that are of interest to you
# In order to use the file make a copy of it an rename it "CustomCMakeLists.txt"

# ----------------------------------
# USEFUL ELEMENTS
# ----------------------------------

# Set the OEM Distribtion if necessary. This will set SKIP flags, so beware!
#SET(OEM_DISTRIBUTION "brainproducts")
#SET(OEM_DISTRIBUTION "mensia")
#SET(OEM_DISTRIBUTION "imotions")
#SET(OEM_DISTRIBUTION "evoke")

# Uncomment the following to build documentation
# both the global project and the specific projects are affected by this token
#SET(SKIP_DOCUMENTATION OFF)
#SET(SKIP_LOG_PACKAGER ON)

#SET(SKIP_GUSBampCAPI ON)

# By default VR demos using Ogre3D are not built, uncomment following line to build them
#SET(SKIP_APPLICATIONS_DEMOS_VR-DEMOS OFF)
#SET(SKIP_APPLICATIONS_DEMOS_SSVEP-DEMO OFF)

# Optionally skip the advanced signal processing tools
#SET(SKIP_EXTERNALS_MENSIA_ADVANCED_SIGNAL_PROCESSING_TOOLS ON)
# By default FastICA plugin is not built, uncomment this line to build it
#SET(SKIP_EXTERNALS_MENSIA_PLUGINS_OVP-PRIVATE-FASTICA OFF)

# Uncomment the following line to enable Experimental signal processing tools
#SET(MENSIA_ADVANCED_SIGNAL_PROCESSING_TOOLS_EXPERIMENTAL ON)

# Compilation of lua code in the Love Demos using luac is active by default, uncomment this line to disable it
#SET(MENSIA_LOVE_DEMOS_LUAC_COMPILATION OFF)

# Uncomment the following line to skip generating Lua demo executables
#SET(SKIP_EXTERNALS_MENSIA_APPLICATIONS_APP-LOVE-DEMO ON)

# By default real advanced plugins are built, uncomment following lines to build ghosts instead
#SET(SKIP_EXTERNALS_MENSIA_PLUGINS_OVP-ADVANCED-VISUALIZATION ON)
#SET(SKIP_EXTERNALS_MENSIA_PLUGINS_OVP-ADVANCED-VISUALIZATION-GHOST OFF)
#SET(SKIP_EXTERNALS_MENSIA_PLUGINS_OVP-ADVANCED-SIGNAL-PROCESSING ON)
#SET(SKIP_EXTERNALS_MENSIA_PLUGINS_OVP-ADVANCED-SIGNAL-PROCESSING-GHOST OFF)

# By default the experimental visualization are not built, uncomment following line to build them
# SET(MENSIA_ADVANCED_VISUALIZATION_EXPERIMENTAL OFF)

# By default real lib-engine is built, if you wish to build the prototype version uncomment following lines
#SET(SKIP_EXTERNALS_MENSIA_PLUGINS_OVP-ENGINE ON)
#SET(SKIP_EXTERNALS_MENSIA_PLUGINS_OVP-ENGINE-PROTO OFF)

# By default scenario-runtime library is skipped, uncomment following line to enable it
#SET(SKIP_EXTERNALS_MENSIA_LIBRARIES_LIB-SCENARIO-RUNTIME OFF)

# By default the engine scenarios are all converted to binary, uncomment to skip this step
#SET(SKIP_MENSIA_ENGINE_SCENARIOS_CONVERSION OFF)

# By default the metabox scenarios are all converted to binary, uncomment to skip this step
#SET(SKIP_MENSIA_METABOXES_CONVERSION OFF)

# By default we skip the private pipelines project, uncomment to enable their installation
#SET(MENSIA_PRIVATE_ENGINE_SERVER_PIPELINE ON)

# By default web-apps are skipped, uncomment following line to enable them
# SET(SKIP_WEB-APPLICATIONS OFF)
# SET(SKIP_EXTERNALS_MENSIA_LIBRARIES_LIB-ARCHWAY-NODE OFF)

